pipeline {

    agent {
        dockerfile {
        filename 'docker/Dockerfile'
        }
    }
    stages {
        stage("build") {
            steps {
                sh '''
                git status # should show <file> as changed or unversioned

                git add $OUTPUT_DIR/src/*
                git commit -m "Added file with post build"
                git push
                '''
            }
        }
    }
}